{% set complete = "true" if applicationValue(["completed", "subject-knowledge"]) %}

{% if showIncomplete and complete != "true" %}
  {{ appSuggestion({
    id: "subject-knowledge",
    title: "Subject knowledge not entered",
    warning: true if errorList,
    actions: {
      items: [{
        text: "What you know about the subject you want to teach?",
        href: applicationPath + "/subject-knowledge/review?referrer=" + referrer
      }]
    }
  }) }}
{% else %}
  {% set summaryBodyHtml %}
    {% include "_includes/item/subject-knowledge.njk" %}
  {% endset %}
  {{ appSummaryCard({
    classes: "govuk-!-margin-bottom-6",
    headingLevel: 3,
    titleText: "Your suitability to teach a subject or age group",
    html: summaryBodyHtml
  }) }}
{% endif %}
