{{ govukSummaryList({
  classes: "govuk-!-font-size-16",
  rows: [{
    key: {
      text: "Answer"
    },
    value: {
      text: (data["interview"] | default("Not entered")) | nl2br | safe
    },
    actions: {
      classes: "govuk-!-padding-right-2",
      items: [{
        href: "/application/interview/edit?referrer=" + referrer,
        text: "Edit",
        visuallyHiddenText: "interview preferences"
      }]
    } if review != true
  }]
}) }}
