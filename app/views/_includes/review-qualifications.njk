{% set qualifications = data["qualifications"] | toArray %}
<table class="govuk-table govuk-!-font-size-16">
  <caption class="govuk-table__caption govuk-heading-m">{{ categoryTitle }}</caption>
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th class="govuk-table__header govuk-!-width-one-half" scope="col">Qualification</th>
      <th class="govuk-table__header govuk-!-width-one-quarter" scope="col">Grade</th>
      <th class="govuk-table__header govuk-!-width-one-quarter" scope="col">Year awarded</th>
      <th class="govuk-table__header" scope="col">Actions</th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
{% for item in qualifications %}
  {% if item.category == category %}
    {% if item.missing %}
    <tr class="govuk-table__row app-table__cell--muted">
      <td class="govuk-table__cell">{{ item.subject }}</td>
      <td class="govuk-table__cell" colspan="2">Missing: {{ item.missing }}</td>
      <td class="govuk-table__cell">
        <a href="/profile/qualifications/{{ category }}/edit/{{ item.id }}">Edit<span class="govuk-visually-hidden"> {{ category}} qualification</span></a>
      </td>
    </tr>
    {% else %}
    <tr class="govuk-table__row">
      <td class="govuk-table__cell">
        {{ item.type }} {{ item.subject }}
        {% if item.country != "United Kingdom" %}
          <br>{{ item.institution }}
        {% endif %}
        {% if item.country != "United Kingdom" %}
          <br>{{ item.country }}
        {% endif %}
      </td>
      <td class="govuk-table__cell">{{ item.grade }}</td>
      <td class="govuk-table__cell">{{ item.year }}</td>
      <td class="govuk-table__cell"><a href="/profile/qualifications/{{ category }}/edit/{{ item.id }}">Edit<span class="govuk-visually-hidden"> {{ category}} qualification</span></a></td>
    </tr>
    {% endif %}
  {% endif %}
{% endfor %}
  </tbody>
</table>
