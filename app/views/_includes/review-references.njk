{% set references = data['referees'] | toArray %}
<table class="govuk-table govuk-!-font-size-16">
  <caption class="govuk-table__caption govuk-heading-m">Your referees</caption>
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th class="govuk-table__header" scope="col">Name</th>
      <th class="govuk-table__header" scope="col">Email</th>
      <th class="govuk-table__header" scope="col">Actions</th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    {% for item in references %}
    <tr class="govuk-table__row">
      <td class="govuk-table__cell">{{ item.name }}</td>
      <td class="govuk-table__cell">{{ item.email }}</td>
      <td class="govuk-table__cell"><a href="/profile/references/edit-referee/{{ item.id }}">Edit referee</a></td>
    </tr>
    {% endfor %}
  </tbody>
</table>
