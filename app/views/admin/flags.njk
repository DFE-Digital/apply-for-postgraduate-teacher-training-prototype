{% extends "_form.njk" %}

{% set parent = "Prototype admin" %}
{% set title = "Feature flags" %}

{% block beforeContent %}
  {{ govukBackLink({
    "href": "javascript: window.history.go(-1)"
  }) }}
{% endblock %}

{% block primary %}
  {% set rows = [] %}
  {% for flag, value in flags %}
    {% set result = rows.push({
      key: {
        classes: "govuk-!-width-three-quarters",
        html: "<code>" + flag + "</code>"
      },
      value: {
        html: "<code>" + value + "</code>"
      }
    }) %}
  {% endfor %}
  {{ govukSummaryList({
    rows: rows
  }) }}
{% endblock %}
