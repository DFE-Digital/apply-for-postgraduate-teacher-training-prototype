{% extends "_form.njk" %}

{% set parent = "Prototype admin" %}
{% set title = "Clear data?" %}
{% set hasAccountLinks = false %}

{% block beforeContent %}
  {{ govukBackLink({
    href: "javascript: window.history.go(-1)"
  }) }}
{% endblock %}

{% block primary %}
  {{ govukWarningText({
    text: "This will clear all of the data entered in this session",
    iconFallbackText: "Warning"
  }) }}

  {{ govukButton({
    text: "Clear the data"
  }) }}
{% endblock %}
